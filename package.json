{
  "name": "sveltekit-starter-prismic-minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --prefix-colors blue,magenta",
    "vite:dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "f -w",
    "format:check": "f",
    "slicemachine": "start-slicemachine"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.2",
    "@iconify-json/ph": "^1.2.2",
    "@magic/format": "^0.0.65",
    "@skeletonlabs/skeleton": "^2.10.4",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@slicemachine/adapter-sveltekit": "^0.3.61",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/kit": "^2.5.15",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^8.56.10",
    "@types/node": "^22.10.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.3",
    "globals": "^15.4.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "slice-machine-ui": "^2.11.1",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.9",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.20",
    "typescript-svelte-plugin": "^0.3.38",
    "unplugin-icons": "^0.22.0",
    "vite": "^5.3.1"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/protest-riot": "^5.1.0",
    "@prismicio/client": "^7.9.0",
    "@prismicio/svelte": "^1.2.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "clsx": "^2.1.1",
    "gsap": "^3.12.5",
    "pocketbase": "^0.24.0"
  }
